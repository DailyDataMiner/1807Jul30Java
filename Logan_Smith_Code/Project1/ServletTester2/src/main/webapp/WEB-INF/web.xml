<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>CoreFilter</filter-name>
  	<display-name>CoreFilter</display-name>
  	<description></description>
  	<filter-class>Filters.CoreFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CoreFilter</filter-name>
  	<url-pattern>/CoreFilter</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>CoreFilter</filter-name>
  	<servlet-name>Dispatcher</servlet-name>
  </filter-mapping>
  <servlet>
  	<servlet-name>Dispatcher</servlet-name>
  	<display-name>Dispatcher</display-name>
  	<description></description>
  	<servlet-class>Servlets.Dispatcher</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Dispatcher</servlet-name>
  	<url-pattern>*.ng</url-pattern>
  </servlet-mapping>
</web-app>
