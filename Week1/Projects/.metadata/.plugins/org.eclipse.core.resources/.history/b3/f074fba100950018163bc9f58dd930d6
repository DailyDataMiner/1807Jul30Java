package com.revature.classbasics;

public class Practice {

	public static void main(String[] args) {
		String str = "leggo my eggo";
		int count = 0;
		
		
			for (int i = 0; i < str.length(); i++)	{
				if (str.substring(i,i+1) == "g") {
					count ++;
				}
					
			}
		System.out.println(count);
		
	}

}
