package com.revature.classbasics;

public class Practice {

	public static void main(String[] args) {
		
		String str = "leggo my eggo";
		int count = 0;
		String str2 = "g";
		
		for (int i = 0; i < str.length(); i++)	{
				if (str.charAt(i) == 'g') {
					count ++;
					System.out.println(count);
				}
					
			}
		System.out.println(count);
		
	}

}
