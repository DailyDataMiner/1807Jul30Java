package com.revature.intro;

public enum Days {
	
	MONDAY(1){
		public void live() {
			System.out.println("Mondays are long days");
		}
	},
	TUEDAY(2){
		public void live() {
			System.out.println("Tequila/Taco tuesday");
		}
	},
	WEDNESDAY(3),
	THURSDAY(4),
	FRIDAY(5),
	SATURDAY(6),
	SUNDAY(7);
	
	Days(int num){
		
	}

}
