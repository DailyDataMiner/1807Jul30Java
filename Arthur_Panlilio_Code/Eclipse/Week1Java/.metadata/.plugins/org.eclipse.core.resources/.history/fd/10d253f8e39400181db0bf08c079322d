package com.revature.structures;

import java.util.ArrayList;

public class controlStructures {

	public static void main(String[] args) {
		int[] nums = {1,2,3,4,5,6,7};
		for(int i = 0; i < nums.length; i++) {
			
			if(nums[i]==4) {
				continue;
			} else {
				System.out.println(nums[i]);
			}
		}

	}
	
	
	public class donkey{
		int id;
		
		donkey(int theId){
			id = theId;
		}
		
	}

}
