package com.revature.q12;

public class Even {

	public static void main(String[] args) {
		int nums[] = new int[101];
		
		for(int i = 1; i < nums.length; i++) {
			nums[i] = i+1;
			System.out.println(nums[i]);
		}
		
		for(int index : nums) {
			if(nums[index] % 2 == 0) {
				System.out.println(nums[index]);
			}
		}
	}

}
