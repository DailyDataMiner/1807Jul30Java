package com.revature.q12;

public class Even {

	public static void main(String[] args) {
		int nums[] = new int[100];
		
		for(int i = 0; i < nums.length; i++) {
			nums[i] = i;
		}
		
		for(int j = 0; j < nums.length; j++) {
			if(nums[j] % 2 == 0) {
				System.out.print(nums[j] + " ");
			}
		}
	}

}
