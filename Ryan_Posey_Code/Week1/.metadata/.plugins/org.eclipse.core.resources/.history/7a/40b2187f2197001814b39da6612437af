package com.revature.q9;

import java.util.ArrayList;

public class Prime {
	
	static ArrayList<Integer> printPrimes(ArrayList<Integer> nums, int size) {
		ArrayList<Integer> primes = new ArrayList<Integer>(size);
		boolean isPrime;
		for(int i = 2; i < size; i++) {
			isPrime = false;
			for(int j = i; j < i/2; j++) {
				if(i % j == 0) {
					isPrime = true;
				}
			}
			if(isPrime) primes.add(nums.get(i));
		}
		return primes;
	}

	public static void main(String[] args) {
		int size = 100;
		ArrayList<Integer> nums = new ArrayList<Integer>(size);
		//System.out.println("hello");
		for(int i = 1; i <= size; i++) {
			nums.add(i);
		}
		System.out.println(nums);
		
		System.out.println(printPrimes(nums, size));
	}

}
