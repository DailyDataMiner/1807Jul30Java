<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>expensereimbursement</display-name>
  <filter>
  	<display-name>CorsFilter</display-name>
  	<filter-name>CorsFilter</filter-name>
  	<filter-class>com.revature.servlet.CorsFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CorsFilter</filter-name>
  	<servlet-name>ServletDispatcher</servlet-name>
  </filter-mapping>
  <servlet>
  	<display-name>ServletDispatcher</display-name>
  	<servlet-name>ServletDispatcher</servlet-name>
  	<servlet-class>com.revature.servlet.ServletDispatcher</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletDispatcher</servlet-name>
  	<url-pattern>*.ng</url-pattern>
  </servlet-mapping>
    
</web-app>