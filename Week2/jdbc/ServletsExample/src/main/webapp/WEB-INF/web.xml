<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>KorsFilter</filter-name>
  	<display-name>KorsFilter</display-name>
  	<description></description>
  	<filter-class>com.ex.servlet.KorsFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>KorsFilter</filter-name>
  	<servlet-name>ServletDispatcher</servlet-name>
  </filter-mapping>
  <servlet>
  	<servlet-name>ServletDispatcher</servlet-name>
  	<display-name>ServletDispatcher</display-name>
  	<description></description>
  	<servlet-class>com.ex.servlet.ServletDispatcher</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletDispatcher</servlet-name>
  	<url-pattern>*.ng</url-pattern>
  </servlet-mapping>
</web-app>
